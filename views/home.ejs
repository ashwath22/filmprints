<!doctype html>
<html>

<head>
  <title>TESTING</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, user-scalable=no, shrink-to-fit=no">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel="stylesheet" href="css/normalize.css">
  <link rel="stylesheet" href="css/html5bp.css">
  <link rel="stylesheet" href="css/filmprints.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fullPage.js/2.8.9/jquery.fullPage.css">
</head>

<body>

<div id="popUp" class="loader hidden">
  <a href="#" class="closePopUp">x</a>  
  <div class="popUpContainer">    
  </div>    
</div>

<section class = "heroSection heroImage">
</section>

<section class = "albumGallery">
</section>

<script>
  $(document).ready(function() {
    $('#fullpage').fullpage();
  });
</script>

<div id="fullpage">
  <div class="section">
    <script id="albumContents" type="text/x-handlebars-template">
      {{#each photo}}
        <div class= "printPhotos section"> 
          <img src="{{link}}" />
          <p>{{location}}</p>
        </div>             
      {{/each}}  
    </script>
  </div>

  <div class="section">
    <script id="heroTemplate" type="text/x-handlebars-template">
      <h1>{{titleText}}</h1>
      <img src="{{heroImageUrl}}" />
      <p>{{leadText}}</p>  
    </script>
  </div>
<!-- </div>
<div id="fullpage"> -->
  <div class="section">
    <script id="albumTemplate" type="text/x-handlebars-template">
      <div class= "albumsSection">
        {{#each album}}
          <div class= "albumTile"> 
            <figure id= {{id}}>
              <h3 class="albumName">{{name}}</h3>
              <img src="{{coverImage}}" />
              <p class="albumDate">{{date}}</p>
            </figure> 
          </div>             
        {{/each}}
      </div>  
    </script>
  </div>
</div>

